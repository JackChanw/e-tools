# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
    . ~/.bashrc
fi

# MacPorts Installer addition on 2014-11-03_at_17:37:15: adding an appropriate PATH variable for use with MacPorts.
PATH=$PATH:$HOME/bin
export PATH
export CLICOLOR=1
export LSCOLORS=GxFxCxDxBxegedabagaced

export PATH=/opt/local/bin:/opt/local/sbin:$PATH
export PATH=${PATH}:/usr/local/mysql/bin:/Library/Python/2.7/site-packages/thrift
export PATH="/opt/local/bin:/opt/local/sbin:$PATH"
export PATH=$PATH:/usr/local/homebrew/bin:/usr/local/homebrew/bin/npm
export PATH=$PATH:/Users/chanw/
# Finished adapting your PATH environment variable for use with MacPorts.

# User specific environment and startup programs
alias 206='mosh chenwei1@10.0.0.206 mosh-command.sh'  #fccGxny
alias mysql='/usr/local/mysql/bin/mysql'
alias sed='/usr/local/homebrew/Cellar/gnu-sed/4.2.2/bin/gsed'
alias ll='ls -l'
alias composer='composer.phar'
alias flushdns='sudo dscacheutil -flushcache'
alias go='/usr/local/go/bin/go'

