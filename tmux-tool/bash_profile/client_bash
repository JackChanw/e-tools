# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
    . ~/.bashrc
fi

# MacPorts Installer addition on 2014-11-03_at_17:37:15: adding an appropriate PATH variable for use with MacPorts.
PATH=$PATH:$HOME/bin
export PATH
export CLICOLOR=1
export LSCOLORS=GxFxCxDxBxegedabagaced


# User specific environment and startup programs
alias 206='mosh chenwei1@10.0.0.206 mosh-command.sh'  #fccGxny

#Managing tmux server

	#tmux    #start tmux server
	#tmux at  #attach running sesseion to terminal
	#tmux ls  #list running tmux sessions
	#
	#exit    #close tmux session
	#
	#Sharing session between terminals
	#tmux new -s session_name  #make new named session
	#tmux at -t session_name    #attach to exists session(s)
	#tmux kill-session -t session_name #killnamed session

